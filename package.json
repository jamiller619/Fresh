{
  "name": "freshdom",
  "version": "0.0.3",
  "description": "A JavaScript library for creating UI on the modern web via Web Components.",
  "main": "lib/index.js",
  "author": {
    "name": "Jeff Miller",
    "website": "https://jamiller.me",
    "email": "hello@jamiller.me",
    "twitter": "jamiller619"
  },
  "license": "MIT",
  "repository": "https://github.com/jamiller619/freshdom.git",
  "scripts": {
    "build": "npm run build-dev && npm run build-prod && npm run build-readme",
    "watch": "rollup -c build/rollup.config.js --environment config:dev --watch",
    "build-dev": "rollup -c build/rollup.config.js --environment config:dev",
    "build-prod": "rollup -c build/rollup.config.js --environment config:prod",
    "build-readme": "mustache package.json build/README.mustache > README.md && mustache package.json build/README.mustache > dist/README.md"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "fastdom": "^1.0.8",
    "fs-extra": "^5.0.0",
    "merge-options": "^1.0.0",
    "morphdom": "^2.3.3",
    "mustache": "^2.3.0",
    "rollup": "^0.56.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-babel-minify": "^3.1.2",
    "rollup-plugin-cleanup": "^1.0.1",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-node-resolve": "^3.0.0",
    "url-pattern": "^1.0.3"
  }
}
